        <div class="row">
            <div class="col-md-3">
                <img src="~/images/callus.avif " class="img-fluid mb-3" alt="Get in Touch">
                <h5 class="text-white">Get in Touch</h5>
                <p class="fw-bold">Call us on<br>......</p>
            </div>
            <div class="col-md-3">
                <img src="~/images/visitOffice.avif" class="img-fluid mb-3" alt="Visit Office">
                <h5 class="text-white">Visit our Office</h5>
            </div>
            <div class="col-md-3">
                <img src="~/images/meetOnline.avif" class="img-fluid mb-3" alt="Meet Online">
                <h5 class="text-white">Meet online</h5>
                <p class="fw-bold">Book an online meeting</p>
            </div>
            <div class="col-md-3">
                <img src="~/images/switchacccountant.avif" class="img-fluid mb-3" alt="Switch Accountant">
                <h5 class="text-white">Switch Accountant</h5>
                <p class="fw-bold">Call us on<br>......</p>
            </div>
        </div>
    </div>
