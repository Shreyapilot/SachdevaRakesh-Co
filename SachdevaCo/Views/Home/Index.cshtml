    <div class="wave-divider-bottom">
        <svg viewBox="0 0 1440 100" xmlns="http://www.w3.org/2000/svg">
            <path fill="#FFFFFF" d="M0,0 C300,100 1140,0 1440,100 L1440,0 L0,0 Z"></path>
        </svg>
    </div>


</section>

<section style="background-color: #0C1533; color: white; height: 500px;" id="articles">
    <div class="container py-4">
        <!-- Heading -->
        <h2 class="text-white mb-5" style="font-family: 'Georgia', serif; font-size: 2.5rem;">Latest Articles</h2>

        <!-- White box with centered text -->
        <div class="bg-white d-flex align-items-center justify-content-center" style="height: 500px;">
            <div class="text-center">
